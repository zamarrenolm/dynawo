<dyn:jobs xmlns:dyn="http://www.rte-france.com/dynawo">
  
  
  <dyn:job name="PQ set point tracking VSC SIM solver">
      <dyn:solver lib="dynawo_SolverSIM" parFile="solvers.par" parId="solverparam"/>
      <dyn:modeler compileDir="outputs_testSIM/compilation">
        <dyn:dynModels dydFile="test.dyd"/>
        <dyn:precompiledModels useStandardModels="true"/>
        <dyn:modelicaModels useStandardModels="true">
          <dyn:directory path="." recursive="false"/>
        </dyn:modelicaModels>
      </dyn:modeler>
      <dyn:simulation startTime="0" stopTime="1"/>
      <dyn:outputs directory="outputs_testSIM">
        <dyn:curves inputFile="test.crv" exportMode="CSV"/>
        <dyn:dumpInitValues local="true" global="true"/>
        <dyn:constraints exportMode="TXT"/>
        <dyn:timeline exportMode="TXT"/>
        <dyn:logs>
          <dyn:appender tag="" file="dynawo.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="NETWORK" file="network.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="PARAMETERS" file="param.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="MODELER" file="modeler.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="COMPILE" file="compile.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="EQUATIONS" file="equations.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="VARIABLES" file="variables.log" lvlFilter="DEBUG"/>
        </dyn:logs>
      </dyn:outputs>
  </dyn:job>
